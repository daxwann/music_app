<div>
  <h1>Sign Up</h1>
  <form action="<%= users_url %>" method="POST">
    <div>
      <label for="email">Email</label>
      <input type="text"
        id="email"
        name="user[email]"
        value="<%= @user.email %>">
    </div> 
    <div>
      <label for="password">Password</label>
      <input type="password"
        id="password"
        name="user[password]"
        value="<%= @user.password %>">
    </div> 
    <div>
      <input type="submit"
        value="Sign Up">
  </form>
</div>
